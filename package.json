{
  "name": "aegis-ui-desktop",
  "version": "1.0.6-2",
  "description": "基于 Vue 私用组件库, 扩展自ant-design-vue",
  "main": "src/packages/index.ts",
  "keyword": "aegisUI aegis-UI aegisui aegis-UI",
  "private": false,
  "scripts": {
    "create": "node node_modules/aegis-ui-tools/dist/create.js && npm run docs",
    "docs": "node node_modules/aegis-ui-tools/dist/check.js && node node_modules/aegis-ui-tools/dist/def.js && node node_modules/aegis-ui-tools/dist/docs.js",
    "dev": "npm run docs && vue-cli-service serve",
    "build": "npm run docs && vue-cli-service build",
    "api": "generate-api",
    "lint": "vue-cli-service lint",
    "lib": "npm run docs && vue-cli-service build --target lib --name aegis --dest lib src/packages/index.ts",
    "remove": "node node_modules/aegis-ui-tools/dist/remove.js && npm run docs",
    "cdn": "tsc -p build && npm run lib && node build/cdn.js",
    "publish": "npm run docs"
  },
  "repository": "https://github.com/wuhao000/aegis-ui-desktop.git",
  "files": [
    "dist",
    "packages",
    "lib",
    "src",
    "README.md",
    "types"
  ],
